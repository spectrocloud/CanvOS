#!/bin/bash
# Image repository for image tagging
IMAGE_REPOSITORY="ttl.sh"

# Environment Options to build
OS_FLAVOR="opensuse-leap"
K8S_FLAVOR="rke2"
K8S_VERSIONS="${K8S_VERSIONS:-1.25.2-k3s1,1.24.6-k3s1}"

# ISO Naming Options
CANVOS_ENV=test
ISO_IMAGE_NAME=$CANVOS_ENV-installer

# Automatically push images to repository
PUSH_BUILD=false
